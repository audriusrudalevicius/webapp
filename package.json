{
  "name": "webapp",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rm -fr dist/",
    "prebuild": "npm run clean",
    "build": "npm run tsc",
    "lint": "npm run tslint \"src/**/*.ts\"",
    "test": "npm run lint && npm run cover",
    "test:mocha": "npm run mocha -- --compilers ts:ts-node/register tests/ --recursive",
    "cover": "tsc && node ./node_modules/istanbul/lib/cli.js cover -x '**/logger.*' _mocha dist/tests/**/*.js && ./node_modules/.bin/remap-istanbul -i ./coverage/coverage.json -o ./coverage/html-report -t html",
    "tsc": "tsc",
    "smid": "smid",
    "mocha": "mocha",
    "tslint": "tslint",
    "istanbul": "istanbul"
  },
  "private": true,
  "engines": {
    "node": ">= 5.4.1"
  },
  "author": "d3trax",
  "license": "MIT",
  "dependencies": {
    "bluebird": "^3.4.1",
    "inversify": "^2.0.0-rc.10",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "reflect-metadata": "^0.1.8",
    "rxjs": "^5.0.0-beta.11"
  },
  "devDependencies": {
    "@types/bluebird": "^3.0.31",
    "@types/chai": "^3.4.31",
    "@types/es6-shim": "0.0.30",
    "@types/inversify": "^2.0.29",
    "@types/mocha": "^2.2.30",
    "@types/node": "^6.0.36",
    "@types/react": "^0.14.31",
    "@types/react-dom": "^0.14.15",
    "@types/selenium-webdriver": "^2.44.27",
    "@types/sinon": "^1.16.28",
    "chai": "^3.5.0",
    "es6-shim": "^0.35.1",
    "istanbul": "^0.4.5",
    "mocha": "^3.0.2",
    "remap-istanbul": "^0.6.4",
    "sinon": "^1.17.5",
    "ts-node": "^1.3.0",
    "tslint": "^3.14.0-dev.1",
    "typescript": "^1.9.0-dev.20160409"
  }
}
